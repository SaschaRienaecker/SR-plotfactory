text.usetex: False
font.family: serif
axes.labelsize: 15
font.size: 11
legend.fontsize: 12
legend.handlelength: 1.5
legend.frameon: True

xtick.labelsize: 8
ytick.labelsize: 8
lines.markersize: 3.5
lines.linewidth: 1.6

ytick.direction: in
xtick.direction: in

savefig.bbox: tight

# Grid lines style
axes.grid: True
grid.color: lightgrey
grid.linestyle: -
grid.linewidth: 0.5
grid.alpha: 0.8